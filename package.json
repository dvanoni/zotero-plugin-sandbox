    "build": "node build.js",
    "clean": "rm -r build gen xpi",
    "generateUpdateManifest": "zotero-plugin-generate-update",
    "postbuild": "zotero-plugin-zipup build sandbox",
    "test": "echo \"Error: no test specified\" && exit 1"
