  },
  "author": {
    "name": "David Hoff-Vanoni",
    "email": "david@vanoni.dev"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dvanoni/zotero-plugin-sandbox/issues"
  },
  "homepage": "https://github.com/dvanoni/zotero-plugin-sandbox#readme",
  "xpi": {
    "name": "Sandbox",
    "releaseURL": "https://github.com/dvanoni/zotero-plugin-sandbox/releases/download/release/",
    "supportsZotero7": true
  },
  "devDependencies": {
    "@dvanoni/zotero-plugin": "^1.5.0"
  }
